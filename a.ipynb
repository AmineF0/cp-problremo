    "s = input().strip()\n",
    "n = int(input().strip())\n",
    "\n",
    "for _ in range(n):\n",
    "    q = input().strip().split(\" \")\n",
    "    if q[0] == \"0\":\n",
    "        index = int(q[1]) - 1\n",
    "        s = s[:index] + q[2] + s[index + 1:]\n",
    "    elif q[0] == \"1\":\n",
    "        idx = s.find(q[1]) + 1\n",
    "        print(idx if idx else -1)"
   ]
  }
